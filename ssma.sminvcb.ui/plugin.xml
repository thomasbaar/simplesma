			class="ssma.sminvcb.ui.SminvcbDslExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="sminvcb"/>
	</extension>
	<!-- START addition TBAAR-->
