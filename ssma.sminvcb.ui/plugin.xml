			class="ssma.sminvcb.ui.SminvcbDslExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
			uriExtension="sminvcb">
		</resourceServiceProvider>
	</extension>
	<!-- marker definitions for ssma.sminvcb.SminvcbDsl -->
	<extension
			id="sminvcbdsl.check.fast"
			name="SminvcbDsl Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="sminvcbdsl.check.normal"
			name="SminvcbDsl Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="sminvcbdsl.check.expensive"
			name="SminvcbDsl Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
